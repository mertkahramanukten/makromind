{
  "name": "fourth-mobile",
  "version": "1.0.0",
  "description": "React Native mobile app for Fourth platform",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace Fourth.xcworkspace -scheme Fourth -configuration Release -destination generic/platform=iOS -archivePath Fourth.xcarchive archive"
  },
  "dependencies": {
    "@fourth/shared-types": "file:../shared-types",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/drawer": "^6.6.3",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/stack": "^6.3.17",
    "axios": "^1.5.0",
    "i18next": "^23.5.1",
    "react": "18.2.0",
    "react-i18next": "^13.2.2",
    "react-native": "0.72.4",
    "react-native-audio-recorder-player": "^3.5.3",
    "react-native-biometrics": "^3.0.1",
    "react-native-calendars": "^1.1302.0",
    "react-native-chart-kit": "^6.12.0",
    "react-native-date-picker": "^4.3.3",
    "react-native-device-info": "^10.8.0",
    "react-native-document-picker": "^9.1.1",
    "react-native-elements": "^3.4.3",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "^2.12.1",
    "react-native-image-picker": "^7.0.3",
